{
  "name": "rentacar-backend",
  "version": "1.0.0",
  "description": "Node.js backend API for RentACar application",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "start:clean": "node utils/kill-port.js && node server/index.js",
    "dev:clean": "node utils/kill-port.js && nodemon server/index.js",
    "seed": "node database/seed.js",
    "test": "jest",
    "test:api": "node test-api.js",
    "test:login": "node test-login.js",
    "test:crud": "node test-crud.js",
    "test:all": "node test-all-endpoints.js",
    "test:complete": "node test-complete-system.js",
    "db:structure": "node utils/show-db-structure.js",
    "restart": "node utils/kill-port.js && npm start"
  },
  "keywords": [
    "rentacar",
    "api",
    "nodejs",
    "express"
  ],
  "author": "RentACar Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
